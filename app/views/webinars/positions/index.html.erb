<p id="notice"><%= notice %></p>

<h1>Webinars Positions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webinars_positions.each do |webinars_position| %>
      <tr>
        <td><%= webinars_position.name %></td>
        <td><%= link_to 'Show', webinars_position %></td>
        <td><%= link_to 'Edit', edit_webinars_position_path(webinars_position) %></td>
        <td><%= link_to 'Destroy', webinars_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Webinars Position', new_webinars_position_path %>
